	<script>
		$(document).ready(function(){
			const inners = document.getElementsByClassName("inner");

		for(let i in inners) {
			const sectionNumber = +i+1;

			const progressBarForInners = document.createElement('div');

			progressBarForInners.classList.add('progress-bar');

			progressBarForInners.classList.add(`section${sectionNumber}`);

			inners[i].appendChild(progressBarForInners);

			const sectionProgressInLocalStorage = localStorage.getItem(`section${sectionNumber}`);

			if (sectionProgressInLocalStorage){
				const progressPercentage = 100*sectionProgressInLocalStorage;
				document.querySelector(`.section${sectionNumber}`).style.width = `${progressPercentage}%`;
			}
		}
		});

	</script>

